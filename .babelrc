{
  "presets": ["env", "react", "stage-0"],
  "plugins": [
    "transform-decorators-legacy",
    [
      "import",
      { "libraryName": "antd", "libraryDirectory": "es", "style": true }
    ],
    "transform-class-properties",
    "transform-runtime",
    "dva-hmr"
  ],
  "env": {
    "production": {
      "plugins": ["transform-remove-console"]
    }
  }
}
